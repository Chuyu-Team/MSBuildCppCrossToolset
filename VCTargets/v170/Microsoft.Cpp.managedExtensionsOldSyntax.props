<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>C++ Common Language Runtime Support, old syntax</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <ExceptionHandling>Async</ExceptionHandling>
      <CompileAsManaged>OldSyntax</CompileAsManaged>
      <BasicRuntimeChecks>Default</BasicRuntimeChecks>
      <DebugInformationFormat Condition="'%(ClCompile.DebugInformationFormatDefined)' != 'true'">ProgramDatabase</DebugInformationFormat>
      <SupportJustMyCode>false</SupportJustMyCode>
      <MinimalRebuild></MinimalRebuild>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <Link>
      <FixedBaseAddress>false</FixedBaseAddress>
      <AssemblyDebug Condition="'$(UseDebugLibraries)' == 'true'">true</AssemblyDebug>
    </Link>
  </ItemDefinitionGroup>
</Project>
