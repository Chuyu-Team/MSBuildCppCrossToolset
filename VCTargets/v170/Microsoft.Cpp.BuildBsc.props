<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Browse Info</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <BrowseInformation>true</BrowseInformation>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <BSCMake>
      <OutputFile>$(OutDir)$(TargetName).bsc</OutputFile>
    </BSCMake>
  </ItemDefinitionGroup>
</Project>
