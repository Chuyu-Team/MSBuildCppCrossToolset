    <Rule.Categories>
      <Category Name="General" DisplayName="常规"/>
    </Rule.Categories>

    <Rule.DataSource>
      <DataSource Persistence="ProjectFile" ItemType="MeshContentTask" Label="" HasConfigurationCondition="true"/>
