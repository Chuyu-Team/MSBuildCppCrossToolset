<Rule Name="ConfigurationLinuxIntelliSense" DisplayName="C/C++" PageTemplate="tool" Description="C/C++" SwitchPrefix="/" Order="100" xmlns="http://schemas.microsoft.com/build/2009/properties">
  <Rule.Categories>
    <Category Name="IntelliSense" DisplayName="IntelliSense" Description="IntelliSense"/>
  </Rule.Categories>
  <Rule.DataSource>
    <DataSource Persistence="ProjectFile"/>
  </Rule.DataSource>
  <StringListProperty Name="NMakeIncludeSearchPath" DisplayName="包含搜索路径" Category="IntelliSense" Switch="I" Description="指定用于解析包含文件的包含搜索路径。" Subtype="folder" F1Keyword="VC.Project.VCNMakeTool.IncludeSearchPath"/>
  <StringListProperty Name="NMakeForcedIncludes" DisplayName="强制包含" Category="IntelliSense" Switch="FI" Description="指定要强制包含的文件。" Subtype="folder" F1Keyword="VC.Project.VCNMakeTool.ForcedIncludes"/>
  <StringListProperty Name="NMakePreprocessorDefinitions" DisplayName="预处理器定义" Category="IntelliSense" Switch="D" Description="指定源文件使用的预处理器定义。" F1Keyword="VC.Project.VCNMakeTool.PreprocessorDefinitions"/>
<!--  <StringListProperty Name="UndefinePreprocessorDefinitions"
                  DisplayName="Undefine Preprocessor Definitions"
                  Description="Specifies one or more preprocessor undefines.     (/U[macro])"
                  Category="IntelliSense"
                  Switch="U"
                  F1Keyword="VC.Project.VCCLCompilerTool.UndefinePreprocessorDefinitions" />-->
  <StringProperty Name="AdditionalOptions" DisplayName="附加选项" Category="IntelliSense" Description="指定分析 C++ 文件时 Intellisense 使用的附加编译器开关"/>
  <StringProperty Name="RemoteCCompileToolExe" DisplayName="C 编译器" Description="指定 C 源文件的编译期间要引用的程序，或远程系统上 C 编译器的路径。" F1Keyword="VC.Project.VCConfiguration.BuildLogFile" Category="IntelliSense" Default="">
    <StringProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label=""/>
    </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="RemoteCppCompileToolExe" DisplayName="C++ 编译器" Description="指定 C++ 源文件的编译期间要引用的程序，或远程系统上 C++ 编译器的路径。" F1Keyword="VC.Project.VCConfiguration.BuildLogFile" Category="IntelliSense" Default="">
    <StringProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label=""/>
    </StringProperty.DataSource>
  </StringProperty>
  <EnumProperty Name="CLanguageStandard" DisplayName="C 语言标准" Description="确定 C 语言标准。" Category="IntelliSense" F1Keyword="VC.Project.VCClangCompilerTool.CLanguageStandard">
    <EnumValue Name="Default" Switch="" DisplayName="默认">
    </EnumValue>
    <EnumValue Name="c89" Switch="std=c89" DisplayName="C89" Description="C89 语言标准。">
    </EnumValue>
    <EnumValue Name="c99" Switch="std=c99" DisplayName="C99" Description="C99 语言标准。">
    </EnumValue>
    <EnumValue Name="c11" Switch="std=c11" DisplayName="C11" Description="C11 语言标准。">
    </EnumValue>
    <EnumValue Name="gnu99" Switch="std=gnu99" DisplayName="C99 (GNU Dialect)" Description="C99 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu11" Switch="std=gnu11" DisplayName="C11 (GNU Dialect)" Description="C11 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumProperty.DataSource>
        <DataSource Persistence="ProjectFile" ItemType="ClCompile" Label="" HasConfigurationCondition="true"/>
    </EnumProperty.DataSource>
  </EnumProperty>
  <EnumProperty Name="CppLanguageStandard" DisplayName="C++ 语言标准" Description="确定 C++ 语言标准。" Category="IntelliSense" F1Keyword="VC.Project.VCClangCompilerTool.CppLanguageStandard">
    <EnumValue Name="Default" Switch="" DisplayName="默认" Description="">
    </EnumValue>
    <EnumValue Name="c++98" Switch="std=c++98" DisplayName="C++98" Description="C++98 语言标准。">
    </EnumValue>
    <EnumValue Name="c++03" Switch="std=c++03" DisplayName="C++03" Description="C++03 语言标准。">
    </EnumValue>
    <EnumValue Name="c++11" Switch="std=c++11" DisplayName="C++11" Description="C++11 语言标准。">
    </EnumValue>
    <EnumValue Name="c++14" Switch="std=c++14" DisplayName="C++14" Description="C++14 语言标准。">
    </EnumValue>
    <EnumValue Name="c++17" Switch="std=c++17" DisplayName="C++17" Description="C++17 语言标准。">
    </EnumValue>
    <EnumValue Name="c++2a" Switch="std=c++2a" DisplayName="C++2a" Description="C++20 语言标准(GCC 9 及更早版本)。">
    </EnumValue>
    <EnumValue Name="c++20" Switch="std=c++20" DisplayName="C++20" Description="C++20 语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++98" Switch="std=gnu++98" DisplayName="C++98 (GNU Dialect)" Description="C++98 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++03" Switch="std=gnu++03" DisplayName="C++03 (GNU Dialect)" Description="C++03 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++11" Switch="std=gnu++11" DisplayName="C++11 (GNU Dialect)" Description="C++11 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++1y" Switch="std=gnu++14" DisplayName="C++14 (GNU Dialect)" Description="C++14 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++20" Switch="std=gnu++20" DisplayName="C++20 (GNU 方言)" Description="C++20 (GNU 方言)语言标准。">
    </EnumValue>
    <EnumProperty.DataSource>
        <DataSource Persistence="ProjectFile" ItemType="ClCompile" Label="" HasConfigurationCondition="true"/>
    </EnumProperty.DataSource>
  </EnumProperty>
</Rule>
