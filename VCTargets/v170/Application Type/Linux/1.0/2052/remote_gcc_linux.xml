<Rule Name="CL" PageTemplate="tool" DisplayName="C/C++" SwitchPrefix="-" Order="10" xmlns="http://schemas.microsoft.com/build/2009/properties" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <Rule.Categories>
    <Category Name="General" DisplayName="常规"/>
    <Category Name="Optimization" DisplayName="优化"/>
    <Category Name="Preprocessor" DisplayName="预处理器"/>
    <Category Name="Code Generation" DisplayName="代码生成"/>
    <Category Name="Language" DisplayName="语言"/>
    <Category Name="Advanced" DisplayName="高级"/>
    <Category Name="All Options" DisplayName="所有选项" Subtype="Search"/>
    <Category Name="Command Line" DisplayName="命令行" Subtype="CommandLine"/>
  </Rule.Categories>
  <Rule.DataSource>
    <DataSource Persistence="ProjectFile" ItemType="ClCompile" Label="" HasConfigurationCondition="true"/>
  </Rule.DataSource>
  <BoolProperty Name="MSVCErrorReport" DisplayName="Visual Studio 错误报告" Description="报告错误，Visual Studio 可以分析文件和行信息。" Category="General" Switch="fdiagnostics-format=msvc" Visible="false" F1Keyword="">
  </BoolProperty>
  <StringProperty Name="GccToolChain" DisplayName="Gcc 工具链" Description="Gcc 工具链的文件夹路径。" Category="General" Switch="gcc-toolchain " Visible="false" F1Keyword="">
  </StringProperty>
  <StringProperty Name="Target" DisplayName="目标" Description="目标" Category="General" Switch="target " Visible="false" F1Keyword="">
  </StringProperty>
  <StringProperty Name="Sysroot" DisplayName="Sysroot" Description="标头和库的根目录的文件夹路径。" Category="General" Switch="-sysroot=" Visible="false" F1Keyword="">
  </StringProperty>
  <StringListProperty Subtype="folder" Name="AdditionalIncludeDirectories" DisplayName="附加包含目录" Description="指定一个或多个要添加到包括路径的目录；如果有多个目录，请用分号分开。（-I[路径]）。" Category="General" Switch="I " F1Keyword="VC.Project.VCClangCompilerTool.AdditionalIncludeDirectories">
  </StringListProperty>
  <StringListProperty Subtype="folder" Name="ProjectPublicIncludePath" DisplayName="公共项目包含目录" Description="指定要添加到包含路径和要通过 AdditionalIncludeDirectories 宏传递到解决方案中的引用项目的一个或多个目录；如果不止一个，请用分号隔开。(-I[path])。" Category="General" Switch="I " F1Keyword="VC.Project.VCClangCompilerTool.AdditionalPublicIncludeDirectories">
    <StringListProperty.DataSource>
       <DataSource Persistence="ProjectFile" Label=""/>
    </StringListProperty.DataSource>
  </StringListProperty>
  <EnumProperty Name="DebugInformationFormat" DisplayName="调试信息格式" Description="指定编译器生成的调试信息的类型。" Category="General" F1Keyword="VC.Project.VCClangCompilerTool.DebugInformationFormat">
    <EnumValue Name="None" Switch="g0" DisplayName="无" Description="没有生成调试信息，因此编译可能会更快。">
    </EnumValue>
    <EnumValue Name="Minimal" Switch="g1" DisplayName="最小调试信息" Description="生成最小调试信息。">
    </EnumValue>
    <EnumValue Name="FullDebug" Switch="g2 -gdwarf-2" DisplayName="全部调试信息 (DWARF2)" Description="生成 DWARF2 调试信息。">
    </EnumValue>
  </EnumProperty>
  <StringProperty Subtype="file" Name="ObjectFileName" DisplayName="对象文件名" Description="指定一个名称以替代默认对象文件名；可以是文件或目录名。(-o [name])。" Category="General" Switch="o " F1Keyword="VC.Project.VCClangCompilerTool.ObjectFile">
  </StringProperty>
  <EnumProperty Name="WarningLevel" DisplayName="警告等级" Description="选择编译器对代码错误的严格程度。其他标志应直接添加到附加选项。(/w, /Weverything)。" Category="General" F1Keyword="VC.Project.VCClangCompilerTool.WarningLevel">
    <EnumValue Name="TurnOffAllWarnings" Switch="w" DisplayName="关闭所有警告" Description="禁用所有编译器警告。">
    </EnumValue>
    <EnumValue Name="EnableAllWarnings" Switch="Wall" DisplayName="启用所有警告" Description="启用所有警告，包括默认情况下禁用的那些警告。">
    </EnumValue>
  </EnumProperty>
  <BoolProperty Name="TreatWarningAsError" DisplayName="将警告视为错误" Description="将所有编译器警告视为错误。对于新项目，最好在所有编译中使用 /Werror；解决所有警告将确保最低可能的难以查找代码缺陷。" Category="General" Switch="Werror" F1Keyword="VC.Project.VCClangCompilerTool.WarnAsError">
  </BoolProperty>
  <StringListProperty Name="CAdditionalWarning" DisplayName="C 其他警告" Description="定义一组其他警告消息。" Category="General" Switch="W" F1Keyword="VC.Project.VCClangCompilerTool.AdditionalWarning"/>
  <StringListProperty Name="CppAdditionalWarning" DisplayName="C++ 其他警告" Description="定义一组其他警告消息。" Category="General" Switch="W" F1Keyword="VC.Project.VCClangCompilerTool.AdditionalWarning"/>
  <BoolProperty Name="Verbose" DisplayName="启用详细模式" Description="启用详细模式后，此工具将打印出更多信息，用于诊断生成。" Category="General" Switch="v" F1Keyword="VC.Project.VCClangCompilerTool.Verbose">
  </BoolProperty>
  <StringProperty Subtype="folder" Name="TrackerLogDirectory" DisplayName="跟踪器日志目录" Description="跟踪器日志目录。" Category="General" IncludeInCommandLine="false" Visible="false">
  </StringProperty>
  <StringProperty Name="RemoteCCompileToolExe" DisplayName="C 编译器" Description="指定 C 源文件的编译期间要引用的程序，或远程系统上 C 编译器的路径。" F1Keyword="VC.Project.VCConfiguration.BuildLogFile" Category="General" Default="">
    <StringProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label=""/>
    </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="RemoteCppCompileToolExe" DisplayName="C++ 编译器" Description="指定 C++ 源文件的编译期间要引用的程序，或远程系统上 C++ 编译器的路径。" F1Keyword="VC.Project.VCConfiguration.BuildLogFile" Category="General" Default="">
    <StringProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label=""/>
    </StringProperty.DataSource>
  </StringProperty>
  <BoolProperty Name="RemoteCompileLocalCopyObjectFiles" DisplayName="复制对象文件" Description="指定是否要将编译对象文件从远程系统复制到本地计算机。" F1Keyword="VC.Project.VCConfiguration.BuildLogFile" Category="General" Default="">
    <BoolProperty.DataSource>
      <DataSource Persistence="ProjectFile" Label=""/>
    </BoolProperty.DataSource>
  </BoolProperty>
  <EnumProperty Name="Optimization" DisplayName="优化" Description="指定应用程序的优化级别。" Category="Optimization" F1Keyword="VC.Project.VCClangCompilerTool.Optimization">
    <EnumValue Name="Custom" Switch="" DisplayName="自定义" Description="自定义优化。">
    </EnumValue>
    <EnumValue Name="Disabled" Switch="O0" DisplayName="已禁用" Description="禁用优化。">
    </EnumValue>
    <EnumValue Name="MinSize" Switch="Os" DisplayName="使大小最小化" Description="优化尺寸。">
    </EnumValue>
    <EnumValue Name="MaxSpeed" Switch="O2" DisplayName="使速度最大化" Description="优化速度。">
    </EnumValue>
    <EnumValue Name="Full" Switch="O3" DisplayName="完全优化" Description="成本高昂的优化。">
    </EnumValue>
  </EnumProperty>
  <BoolProperty ReverseSwitch="fno-strict-aliasing" Name="StrictAliasing" DisplayName="严格别名" Description="假设使用最严格的别名检查规则。一种类型的对象将始终不会被假定驻留在与另一种类型的对象相同的位置。" Category="Optimization" Switch="fstrict-aliasing" F1Keyword="VC.Project.VCClangCompilerTool.StrictAliasing">
  </BoolProperty>
  <BoolProperty Name="UnrollLoops" DisplayName="展开循环" Description="展开循环，以便以更大的代码大小为代价减少执行的分支数，从而使应用程序更快。" Category="Optimization" Switch="funroll-all-loops" F1Keyword="VC.Project.VCClangCompilerTool.UnrollLoops">
  </BoolProperty>
  <BoolProperty Name="LinkTimeOptimization" DisplayName="链接时间优化" Description="通过允许优化器跨应用程序中的对象文件进行查看，来实现过程间优化。" Category="Optimization" Switch="flto" F1Keyword="VC.Project.VCClangCompilerTool.LinkTimeOptimization">
  </BoolProperty>
  <BoolProperty ReverseSwitch="fno-omit-frame-pointer" Name="OmitFramePointers" DisplayName="省略帧指针" Description="禁止在调用堆栈上创建帧指针。" Category="Optimization" Switch="fomit-frame-pointer" F1Keyword="VC.Project.VCClangCompilerTool.OmitFramePointers">
  </BoolProperty>
  <BoolProperty Name="NoCommonBlocks" DisplayName="无公共块" Description="在对象文件的数据节中分配甚至未初始化的全局变量，而不是以公共块的形式生成它们" Category="Optimization" Switch="fno-common" F1Keyword="VC.Project.VCClangCompilerTool.NoCommonBlocks">
  </BoolProperty>
  <EnumProperty Name="EnableAVE" DisplayName="启用其他矢量扩展" Description="启用扩展矢量指令的使用。仅当以 Intel 体系结构为目标时才使用" Category="Code Generation" F1Keyword="VC.Project.VCClangCompilerTool.EnableAVE" Visible="false">
    <EnumValue Name="SSE3" Switch="msse3" DisplayName="SSE3 指令集" Description="启用 SSE3 指令集。">
    </EnumValue>
    <EnumValue Name="SSE3 (with supplemental extensions)" Switch="mssse3" DisplayName="SSE3 补充指令集" Description="启用 SSE3 指令集。">
    </EnumValue>
    <EnumValue Name="SSE41" Switch="msse4.1" DisplayName="SSE4.1 指令集" Description="启用 SSE41 指令集。">
    </EnumValue>
    <EnumValue Name="SSE42" Switch="msse4.2" DisplayName="SSE4.2 指令集" Description="启用 SSE42 指令集。">
    </EnumValue>
    <EnumValue Name="AVX" Switch="mavx" DisplayName="AVX 指令集" Description="启用 AVX 指令集。">
    </EnumValue>
    <EnumValue Name="AVX2" Switch="mavx2" DisplayName="AVX2 指令集" Description="启用 AVX2 指令集。">
    </EnumValue>
  </EnumProperty>
  <BoolProperty Name="PositionIndependentCode" DisplayName="位置无关代码" Description="生成位置无关代码(PIC)以便在共享库中使用。" Category="Code Generation" Switch="fpic" F1Keyword="VC.Project.VCClangCompilerTool.PIC">
  </BoolProperty>
<!--  <BoolProperty Name="MakeStringsReadOnly" DisplayName="Make Strings Read Only" Description="Reuse String Literals" Category="Code Generation" Switch="fwritable-strings" F1Keyword="VC.Project.VCClangCompilerTool.MakeStringsReadOnly" Visible="false">
  </BoolProperty>-->
  <!-- <BoolProperty Name="ThreadSafeStatics" DisplayName="Statics are thread safe" Description="Emit Extra code to use routines specified in C++ ABI for thread safe initilization of local statics." Category="Code Generation" Switch="fno-threadsafe-statics" F1Keyword="VC.Project.VCClangCompilerTool.ThreadSafeStatics">
  </BoolProperty> -->
  <EnumProperty Name="ThreadSafeStatics" DisplayName="静态变量是线程安全的" Description="发出额外代码以使用 C++ ABI 中指定的例程实现局部静态变量的线程安全初始化。" Category="Code Generation" F1Keyword="VC.Project.VCClangCompilerTool.ThreadSafeStatics">
    <EnumValue Name="No" Switch="fno-threadsafe-statics" DisplayName="否" Description="禁用线程安全的静态变量。">
    </EnumValue>
    <EnumValue Name="Yes" Switch="fthreadsafe-statics" DisplayName="是" Description="启用线程安全的静态变量。">
    </EnumValue>
  </EnumProperty>
  <BoolProperty Name="RelaxIEEE" DisplayName="浮点优化" Description="通过放宽 IEEE-754 合规性来实现浮点优化。" Category="Code Generation" Switch="ffast-math" F1Keyword="VC.Project.VCClangCompilerTool.RelaxIEEE">
  </BoolProperty>
  <BoolProperty Name="HideInlineMethods" DisplayName="内联方法已隐藏" Description="启用时，内联方法的外联副本会声明为“private extern”。" Category="Code Generation" Switch="fvisibility-inlines-hidden" F1Keyword="VC.Project.VCClangCompilerTool.HideInlineMethods">
  </BoolProperty>
  <BoolProperty Name="SymbolsHiddenByDefault" DisplayName="符号在默认情况下隐藏" Description="所有符号都声明为“private extern”，除非显式标记为使用“__attribute”宏导出。" Category="Code Generation" Switch="fvisibility=hidden" F1Keyword="VC.Project.VCClangCompilerTool.SymbolsHiddenByDefault">
  </BoolProperty>
  <EnumProperty Name="ExceptionHandling" DisplayName="启用 C++ 异常" Description="指定将由编译器使用的异常处理模型。" Category="Code Generation" F1Keyword="VC.Project.VCClangCompilerTool.ExceptionHandling">
    <EnumValue Name="Disabled" Switch="fno-exceptions" DisplayName="否" Description="禁用异常处理。">
    </EnumValue>
    <EnumValue Name="Enabled" Switch="fexceptions" DisplayName="是" Description="启用异常处理。">
    </EnumValue>
  </EnumProperty>
  <BoolProperty ReverseSwitch="fno-rtti" Name="RuntimeTypeInfo" DisplayName="启用运行时类型信息" Description="添加代码，用于检查运行时的 C++ 对象(运行时类型信息)。     (frtti, fno-rtti)" Category="Language" Switch="frtti" F1Keyword="VC.Project.VCClangCompilerTool.RuntimeTypeInfo">
  </BoolProperty>
  <EnumProperty Name="CLanguageStandard" DisplayName="C 语言标准" Description="确定 C 语言标准。" Category="Language" F1Keyword="VC.Project.VCClangCompilerTool.CLanguageStandard">
    <EnumValue Name="Default" Switch="" DisplayName="默认">
    </EnumValue>
    <EnumValue Name="c89" Switch="std=c89" DisplayName="C89" Description="C89 语言标准。">
    </EnumValue>
    <EnumValue Name="c99" Switch="std=c99" DisplayName="C99" Description="C99 语言标准。">
    </EnumValue>
    <EnumValue Name="c11" Switch="std=c11" DisplayName="C11" Description="C11 语言标准。">
    </EnumValue>
    <EnumValue Name="gnu99" Switch="std=gnu99" DisplayName="C99 (GNU Dialect)" Description="C99 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu11" Switch="std=gnu11" DisplayName="C11 (GNU Dialect)" Description="C11 (GNU Dialect)语言标准。">
    </EnumValue>
  </EnumProperty>
  <EnumProperty Name="CppLanguageStandard" DisplayName="C++ 语言标准" Description="确定 C++ 语言标准。" Category="Language" F1Keyword="VC.Project.VCClangCompilerTool.CppLanguageStandard">
    <EnumValue Name="Default" Switch="" DisplayName="默认" Description="">
    </EnumValue>
    <EnumValue Name="c++98" Switch="std=c++98" DisplayName="C++98" Description="C++98 语言标准。">
    </EnumValue>
    <EnumValue Name="c++03" Switch="std=c++03" DisplayName="C++03" Description="C++03 语言标准。">
    </EnumValue>
    <EnumValue Name="c++11" Switch="std=c++11" DisplayName="C++11" Description="C++11 语言标准。">
    </EnumValue>
    <EnumValue Name="c++14" Switch="std=c++14" DisplayName="C++14" Description="C++14 语言标准。">
    </EnumValue>
    <EnumValue Name="c++17" Switch="std=c++17" DisplayName="C++17" Description="C++17 语言标准。">
    </EnumValue>
    <EnumValue Name="c++2a" Switch="std=c++2a" DisplayName="C++2a" Description="C++20 语言标准(GCC 9 及更早版本)。">
    </EnumValue>
    <EnumValue Name="c++20" Switch="std=c++20" DisplayName="C++20" Description="C++20 语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++98" Switch="std=gnu++98" DisplayName="C++98 (GNU Dialect)" Description="C++98 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++03" Switch="std=gnu++03" DisplayName="C++03 (GNU Dialect)" Description="C++03 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++11" Switch="std=gnu++11" DisplayName="C++11 (GNU Dialect)" Description="C++11 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++1y" Switch="std=gnu++14" DisplayName="C++14 (GNU Dialect)" Description="C++14 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++17" Switch="std=gnu++17" DisplayName="C++17 (GNU Dialect)" Description="C++17 (GNU Dialect)语言标准。">
    </EnumValue>
    <EnumValue Name="gnu++20" Switch="std=gnu++20" DisplayName="C++20 (GNU 方言)" Description="C++20 (GNU 方言)语言标准。">
    </EnumValue>
  </EnumProperty>
  <StringListProperty Name="PreprocessorDefinitions" DisplayName="预处理器定义" Description="定义源文件的预处理符号。(-D)" Category="Preprocessor" Switch="D " F1Keyword="VC.Project.VCClangCompilerTool.PreprocessorDefinitions">
  </StringListProperty>
  <StringListProperty Name="UndefinePreprocessorDefinitions" DisplayName="取消预处理器定义" Description="指定取消一个或多个预处理器定义。  (-U [宏])" Category="Preprocessor" Switch="U " F1Keyword="VC.Project.VCClangCompilerTool.UndefinePreprocessorDefinitions">
  </StringListProperty>
  <BoolProperty Name="UndefineAllPreprocessorDefinitions" DisplayName="取消所有预处理器定义" Description="取消以前定义的所有预处理器值。(-undef)" Category="Preprocessor" Switch="undef" F1Keyword="VC.Project.VCClangCompilerTool.UndefineAllPreprocessorDefinitions">
  </BoolProperty>
  <BoolProperty Name="ShowIncludes" DisplayName="显示包含文件" Description="生成包含文件的列表(包括编译器输出)。  (-H)" Category="Preprocessor" Switch="H" F1Keyword="VC.Project.VCClangCompilerTool.ShowIncludes">
  </BoolProperty>
