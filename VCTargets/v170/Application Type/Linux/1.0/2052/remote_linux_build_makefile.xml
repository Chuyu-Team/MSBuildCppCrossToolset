<Rule Name="LinuxRemoteBuild" DisplayName="远程生成" PageTemplate="generic" Description="远程生成" SwitchPrefix="/" Order="100" xmlns="http://schemas.microsoft.com/build/2009/properties">
  <Rule.Categories>
    <Category Name="General" DisplayName="常规" Description="常规"/>
  </Rule.Categories>
  <Rule.DataSource>
    <DataSource Persistence="ProjectFile"/>
  </Rule.DataSource>
  <StringProperty Name="RemoteBuildCommandLine" DisplayName="生成命令行" Description="指定要为“生成”命令运行的命令行。这可在远程系统上执行。" IncludeInCommandLine="false" Category="General" F1Keyword="VC.Project.VCNMakeTool.BuildCommandLine">
    <StringProperty.ValueEditors>
      <ValueEditor EditorType="DefaultCommandPropertyEditor" DisplayName="&lt;编辑...&gt;"/>
    </StringProperty.ValueEditors>
  </StringProperty>
  <StringProperty Name="RemoteReBuildCommandLine" DisplayName="重新生成所有命令行" Description="指定要为“全部重新生成”命令运行的命令行。这可在远程系统上执行。" IncludeInCommandLine="false" Category="General" F1Keyword="VC.Project.VCNMakeTool.ReBuildCommandLine">
    <StringProperty.ValueEditors>
      <ValueEditor EditorType="DefaultCommandPropertyEditor" DisplayName="&lt;编辑...&gt;"/>
    </StringProperty.ValueEditors>
  </StringProperty>
  <StringProperty Name="RemoteCleanCommandLine" DisplayName="清除命令行" Description="指定要为“清除”命令运行的命令行。这可在远程系统上执行。" IncludeInCommandLine="false" Category="General" F1Keyword="VC.Project.VCNMakeTool.CleanCommandLine">
    <StringProperty.ValueEditors>
      <ValueEditor EditorType="DefaultCommandPropertyEditor" DisplayName="&lt;编辑...&gt;"/>
    </StringProperty.ValueEditors>
  </StringProperty>
  <StringListProperty Name="RemoteBuildOutputs" DisplayName="输出" Description="指定远程系统上由远程生成生成的输出。" Category="General" IncludeInCommandLine="false" F1Keyword="VC.Project.VCNMakeTool.Output"/>
  <StringListProperty Subtype="folder" Name="ProjectPublicIncludePath" DisplayName="项目公共包含目录" Description="指定要传递到引用项目的一个或多个目录；如果不止一个，请用分号隔开。" IncludeInCommandLine="false" Category="General" F1Keyword="VC.Project.VCClangCompilerTool.AdditionalPublicIncludeDirectories"/>
</Rule>
