<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.MSVC.Toolset.Common.props"/>
  
  <PropertyGroup>
    <ExecutablePath Condition="'$(ExecutablePath)' == ''">$(VC_ExecutablePath_ARM);$(CommonExecutablePath)</ExecutablePath>
    <ReferencePath Condition="'$(ReferencePath)' == ''">$(VC_ReferencesPath_ARM);</ReferencePath>
    <LibraryPath Condition="'$(LibraryPath)' == ''">$(VC_LibraryPath_ARM);$(WindowsSDK_LibraryPath_ARM)</LibraryPath>
    <ExcludePath Condition="'$(ExcludePath)' == ''">$(CommonExcludePath);$(VC_ExecutablePath_ARM);$(VC_LibraryPath_ARM)</ExcludePath>
    <DebugCppRuntimeFilesPath>$(VC_DebugCppRuntimeFilesPath_ARM);$(UniversalDebugCRT_ExecutablePath_ARM)</DebugCppRuntimeFilesPath>
    <CppRuntimeFilesPath>$(VC_CppRuntimeFilesPath_ARM);</CppRuntimeFilesPath>
  </PropertyGroup>
 </Project>
